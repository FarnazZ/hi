#"\ek": kill-line
#"\er": reverse-search-history
#"\ep": previous-history
#"\en": next-history
#"\ej": accept-line
#"\ea": beginning-of-line
#"\ee": end-of-line
#"\eh": backward-char
#"\el": forward-char
#"\ed": backward-delete-char
#"\ef": delete-char
